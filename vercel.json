{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/install",
      "dest": "/api/install"
    },
    {
      "src": "/status",
      "dest": "/api/status"
    },
    {
      "src": "/test",
      "dest": "/api/test"
    },
    {
      "src": "/auth",
      "dest": "/api/auth"
    },
    {
      "src": "/auth/callback",
      "dest": "/api/auth/callback"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}