{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/install",
      "dest": "/api/install.js"
    },
    {
      "src": "/auth",
      "dest": "/api/auth.js"
    },
    {
      "src": "/auth/callback",
      "dest": "/api/auth/callback.js"
    },
    {
      "src": "/api/upload",
      "dest": "/api/upload.js"
    },
    {
      "src": "/api/transform",
      "dest": "/api/transform.js"
    },
    {
      "src": "/api/products",
      "dest": "/api/products.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}